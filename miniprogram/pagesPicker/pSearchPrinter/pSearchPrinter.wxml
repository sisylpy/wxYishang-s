


<view>{{ppp}}</view>

  <view  wx:for="{{list}}" wx:for-item="item" 
         data-title="{{item.deviceId}}" 
         data-name="{{item.name}}" 
         data-advertisData="{{item.advertisServiceUUIDs}}" 
         wx:key="{{item.deviceId}}"
         bindtap="bindViewTap">
      <view  class="item">
      <div class="deviceId block">{{item.deviceId}}</div>
      <div class="name block">{{item.name}}</div>
     </view>    
</view>  
 
